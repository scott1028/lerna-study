  "scripts": {
    "edit": "cross-env wait-on ./dist/swagger.yaml && swagger_swagger_editorDir=node_modules/swagger-editor swagger_swagger_fileName=dist/swagger.yaml swagger project edit",
    "build": "gulp webpack",
    "watch": "gulp",
    "api": "wait-on ./dist/app.dist.js && node ./dist/app.dist.js",
    "api:debug": "wait-on ./dist/app.dist.js && node --inspect-brk ./dist/app.dist.js",
    "lift:edit": "npm-run-all -p watch api edit",
    "lift": "npm-run-all -p watch api",
    "prelift:edit": "cross-env rm -rf dist public",
    "prelift": "cross-env rm -rf dist public"
  },
